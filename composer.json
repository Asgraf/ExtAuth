{
    "name": "sc0ttyd/ext-auth",
    "description": "CakePHP federated login / external authentication (via OAuth1 and OAuth2) Plugin",
    "version": "0.1.0",
    "type": "cakephp-plugin",
    "keywords": ["cakephp","oauth","auth","extauth"],
    "homepage": "https://github.com/sc0ttyd/ExtAuth",
    "time": "2013-06-14 00:45:00",
    "license": "MIT",
    "authors": [
        {
            "name": "Scott Donnelly",
            "homepage": "http://scott.donnel.ly",
            "role": "Developer"
        }
    ],
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "googlecode/OAuth",
                "description": "Andy Smith's PHP OAuth Library",
                "version": "1.0.1272",
                "license": "MIT",
                "authors": [
                    {
                        "name": "Andy Smith"
                    }
                ],
                "source": {
                    "url": "http://oauth.googlecode.com/svn/",
                    "type": "svn",
                    "reference": "code/php/"
                },
                "autoload": {
                    "classmap": ["OAuth.php"]

                }
            }
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "composer/installers": "*",
        "googlecode/OAuth": "*"
    }
}